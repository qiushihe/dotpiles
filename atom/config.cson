"*":
  "atom-beautify":
    _analyticsUserId: "ab99a765-2c04-4af5-9aab-ba865d93636e"
  core:
    audioBeep: false
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    fontFamily: "Monaco"
    invisibles: {}
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
  "exception-reporting":
    userId: "0915b6e6-7d53-bdfb-6654-245ce3afb932"
  "expand-region": {}
  "file-icons":
    coloured: false
  "highlight-line":
    hideHighlightOnSelect: true
  "highlight-selected": {}
  "merge-conflicts":
    gitPath: "/usr/bin/git"
  minimap:
    plugins:
      bookmarks: true
      "find-and-replace": true
      "git-diff": true
      "highlight-selected": true
      pigments: true
      selection: true
  "one-dark-ui": {}
  pigments: {}
  "status-bar": {}
  tabs:
    usePreviewTabs: true
  "vim-mode": {}
  welcome:
    showOnStartup: false
".ini.source":
  editor:
    preferredLineLength: 100
    scrollPastEnd: true
