    fontSize: 14
  "one-dark-ui": {}
  tabs:
    usePreviewTabs: true
  "file-icons":
    coloured: false
  "highlight-line":
    hideHighlightOnSelect: true
  "highlight-selected": {}
  "status-bar": {}
  minimap:
    plugins:
      "git-diff": true
      "highlight-selected": true
      selection: true
      bookmarks: true
      pigments: true
      "find-and-replace": true
  "expand-region": {}
".ini.source":
  editor:
    preferredLineLength: 100
